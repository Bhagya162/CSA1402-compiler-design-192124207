#include<stdio.h>
int main()
{
	char input[5];
	
	printf("enter an operator:");
	scanf("%s",input);
	switch(input[0])
	{
		case'>':
			if(input[1]=='=')
			   printf("greater than or equal");
			else
			   printf("greater than");
			break;
		case'<':
			if(input[1]=='=')
			  printf("less than or equal");
			else
			   printf("less than");
			break;
		case'!':
			if(input[1]=='=')
			  printf("not equal");
			else
			   printf("bitwise not");
			break;
		case'&':
			if(input[1]=='&')
			  printf("logical AND");
			else
			   printf("bitwise AND");
			break;
		case'|':
			if(input[1]=='|')
			  printf("logical OR");
			else
			   printf("bitwise OR");
			break;
		case'+':
			printf("addition");
			break;
		case'-':
			printf("subtraction");
			break;
		case'*':
			printf("multiplication");
			break;
		case'/':
			printf("division");
			break;
		default:
			printf("not a operator");
	}
	
	return 0;
}
